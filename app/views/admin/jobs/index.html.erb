<p style="color: green"><%= notice %></p>

<h1>Jobs</h1>

<div id="jobs">
  <% @presenter.jobs.each do |job| %>
    <%= render job %>
    <p>
      <%= link_to "Show this job", admin_job_path(job) %>
    </p>
  <% end %>
</div>

<%= link_to "New job", new_admin_job_path, :class => 'btn btn-primary text-white' %>
